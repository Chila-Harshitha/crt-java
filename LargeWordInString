class WordCount{
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
    String str=s.nextLine();
    String res[]=str.split(" ");
int max=0;
String s4="";
for(int i=0;i<res.length;i++){
int len=res[i].length();
if(max<len){
s4=res[i];
}
}
    System.out.println(s4);
}
}
