import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String s1=s.next();
        int l=s1.length();
        for(int i=l-1;i>=0;i--)
        System.out.print(s1.charAt(i));
        System.out.println();
        char s2[]=s1.toCharArray();
        for(int i=l-1;i>=0;i--)
        System.out.print(s2[i]);
    }
}
